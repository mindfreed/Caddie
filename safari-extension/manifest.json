{
  "manifest_version": 2,
  "name": "Freedmind AI Caddy",
  "version": "1.0.0",
  "description": "Strategic intelligence companion for mobile gamers.",
  "permissions": ["clipboardRead", "activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["overlay.css"],
      "run_at": "document_end"
    }
  ],
  "browser_specific_settings": {
    "safari": {
      "strict_min_version": "14.0"
    }
  }
}
